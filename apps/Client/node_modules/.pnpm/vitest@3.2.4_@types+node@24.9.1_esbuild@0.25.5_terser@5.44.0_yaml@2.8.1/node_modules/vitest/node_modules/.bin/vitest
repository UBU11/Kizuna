#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubu/Documents/Web/kizuna/App/Client/node_modules/.pnpm/vitest@3.2.4_@types+node@24.9.1_esbuild@0.25.5_terser@5.44.0_yaml@2.8.1/node_modules/vitest/node_modules:/home/ubu/Documents/Web/kizuna/App/Client/node_modules/.pnpm/vitest@3.2.4_@types+node@24.9.1_esbuild@0.25.5_terser@5.44.0_yaml@2.8.1/node_modules:/home/ubu/Documents/Web/kizuna/App/Client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubu/Documents/Web/kizuna/App/Client/node_modules/.pnpm/vitest@3.2.4_@types+node@24.9.1_esbuild@0.25.5_terser@5.44.0_yaml@2.8.1/node_modules/vitest/node_modules:/home/ubu/Documents/Web/kizuna/App/Client/node_modules/.pnpm/vitest@3.2.4_@types+node@24.9.1_esbuild@0.25.5_terser@5.44.0_yaml@2.8.1/node_modules:/home/ubu/Documents/Web/kizuna/App/Client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
